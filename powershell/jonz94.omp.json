{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#EACA8D",
          "background": "#343A43",
          "trailing_diamond": "\uE0B0"
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#343A43",
          "background": "#71BDF2",
          "properties": {
            "style": "agnoster_full",
            "prefix": " \uF07C  "
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#343A43",
          "background": "#A7CB8B",
          "properties": {
            "display_status": true,
            "display_status_detail": true,
            "display_stash_count": true,
            "branch_icon": " ",
            "local_working_icon": "  ",
            "local_staged_icon": "  ",
            "stash_count_icon": "\u25CF ",
            "tag_icon": "\uF412 ",
            "display_upstream_icon": true,
            "github_icon": "  ",
            "git_icon": " "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#000000",
          "background": "#FFFFFF",
          "properties": {
            "prefix": "",
            "text": " ",
            "postfix": ""
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FFFFFF",
          "background": "#000000",
          "properties": {
            "prefix": "",
            "text": "\uE0B0",
            "postfix": ""
          }
        }
      ]
    }
  ],
  "final_space": true
}
