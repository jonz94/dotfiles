{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#ffff66",
          "background": "#343A43",
          "properties": {
            "root_icon": "",
            "postfix": ""
          }
        },
        {
          "type": "session",
          "style": "powerline",
          "foreground": "#EACA8D",
          "background": "#343A43"
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#343A43",
          "background": "#71BDF2",
          "properties": {
            "style": "agnoster_full",
            "prefix": "   "
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#343A43",
          "background": "#A7CB8B",
          "properties": {
            "display_status": true,
            "display_status_detail": true,
            "display_stash_count": true,
            "branch_icon": " ",
            "local_working_icon": "  ",
            "local_staged_icon": "  ",
            "stash_count_icon": "● ",
            "tag_icon": "​ ",
            "display_upstream_icon": true,
            "github_icon": "  ",
            "git_icon": " "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "display_exit_code": true,
            "always_enabled": false,
            "error_color": "#e91e63",
            "color_background": true
          }
        },
        {
          "type": "battery",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#343A43",
          "properties": {
            "postfix": "% "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#000000",
          "background": "#FFFFFF",
          "properties": {
            "prefix": "",
            "text": " ",
            "postfix": ""
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#FFFFFF",
          "properties": {
            "prefix": "",
            "text": "",
            "postfix": ""
          }
        }
      ]
    }
  ],
  "final_space": true
}
