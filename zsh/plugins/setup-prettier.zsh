# setup prettier

# npm
nip() {
  npm i --save-dev prettier prettier-plugin-packagejson prettier-plugin-organize-imports @types/prettier
}

# pnpm
pnip() {
  pnpm i --save-dev prettier prettier-plugin-packagejson prettier-plugin-organize-imports @types/prettier
}

# yarn
yap() {
  yarn add --dev prettier prettier-plugin-packagejson prettier-plugin-organize-imports @types/prettier
}
