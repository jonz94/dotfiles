{
  "Ionic DI Snippets": {
    "prefix": ";snippets-ionic-di",
    "body": [
      "\"ionic $1 DI\": {",
      "  \"prefix\": \";di-ionic-${1/\\ /-/g}\",",
      "  \"body\": [\"private ${1/(.*)/${1:/camelcase}/}: ${1/(.*)/${1:/pascalcase}/}\\$1, \\$0\"]",
      "},"
    ]
  },
  "Service DI Snippets": {
    "prefix": ";snippets-service-di",
    "body": [
      "\"$1 service DI\": {",
      "  \"prefix\": \";di-${1/\\ /-/g}-service\",",
      "  \"body\": [\"private ${1/(.*)/${1:/camelcase}/}Service: ${1/(.*)/${1:/pascalcase}/}Service\\$1, \\$0\"]",
      "},"
    ]
  },

  "just placed as the last item to conserve the last snippets' tailing comma": {
    "prefix": "Éµ",
    "body": ""
  }
}
